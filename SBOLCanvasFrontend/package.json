{
    "name": "sbolcanvas-frontend",
    "version": "0.0.0",
    "scripts": {
        "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/SBOLCanvasFrontend/stats.json",
        "prebuild": "npm rebuild node-sass && ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" git.version.ts && node assetBundler.js",
        "build": "ng build --prod --build-optimizer --vendor-chunk --progress --output-hashing=all --stats-json --source-map=true --base-href=/canvas/",
        "predev": "npm run prebuild",
        "dev": "ng serve -o",
        "gitversion": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" git.version.ts"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.2.12",
        "@angular/cdk": "^7.3.7",
        "@angular/common": "^7.2.15",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "^7.2.15",
        "@angular/flex-layout": "^7.0.0-beta.24",
        "@angular/forms": "~7.2.0",
        "@angular/material": "^7.3.7",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@ng-bootstrap/ng-bootstrap": "^4.1.1",
        "bootstrap": "^4.3.1",
        "core-js": "^2.5.4",
        "file-saver": "^2.0.2",
        "hammerjs": "^2.0.8",
        "handlebars": "^4.7.8",
        "jquery": "^3.5.0",
        "material-community-components": "^3.4.0",
        "minimist": "^1.2.8",
        "mkdirp": "^0.5.6",
        "mxgraph": "^3.9.12",
        "nanoid": "^4.0.0",
        "nanoid-dictionary": "^4.3.0",
        "ng-sidebar": "^8.0.0",
        "ngx-bootstrap": "^4.0.0",
        "popper.js": "^1.15.0",
        "rxjs": "^6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~17.0.6",
        "@angular/cli": "~7.3.7",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/file-saver": "^2.0.1",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "^2.0.7",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~6.4.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "node-sass": "^4.13.1",
        "protractor": "~5.4.4",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2",
        "webpack-bundle-analyzer": "^3.6.0"
    },
    "engines": {
        "npm": "<7"
    }
}