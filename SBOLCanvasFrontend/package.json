{
  "name": "sbolcanvas-frontend",
  "version": "0.0.0",
  "scripts": {
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/SBOLCanvasFrontend/stats.json",
    "prebuild": "npm rebuild node-sass && ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" git.version.ts && node assetBundler.js",
    "build": "ng build --prod --build-optimizer --vendor-chunk --progress --output-hashing=all --stats-json --source-map=true --base-href=/canvas/",
    "predev": "npm run prebuild",
    "dev": "ng serve -o",
    "gitversion": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" git.version.ts",
    "postinstall": "patch-package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "~18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "~18.0.6",
    "@angular/localize": "^18.0.6",
    "@angular/material": "^18.0.6",
    "@angular/platform-browser": "~18.0.6",
    "@angular/platform-browser-dynamic": "~18.0.6",
    "@angular/router": "~18.0.6",
    "file-saver": "^2.0.5",
    "mxgraph": "^3.9.12",
    "nanoid": "^5.0.7",
    "nanoid-dictionary": "^4.3.0",
    "ngx-color-picker": "^17.0.0",
    "patch-package": "^8.0.0",
    "rxjs": "^7.8.1",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "~18.0.6",
    "@angular/language-service": "~18.0.6",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "~20.14.10",
    "codelyzer": "~6.0.2",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "node-sass": "^9.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.3",
    "typescript": "~5.4.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
