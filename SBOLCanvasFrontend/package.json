    "mxgraph": "^3.9.12",
    "nanoid": "^4.0.0",
    "nanoid-dictionary": "^4.3.0",
    "ng-sidebar": "^8.0.0",
    "ngx-bootstrap": "^4.0.0",
    "nvm": "^0.0.4",
